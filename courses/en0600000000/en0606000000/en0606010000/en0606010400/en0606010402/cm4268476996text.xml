<component id="ID_clip1_txt13">
   <text><![CDATA[<b>is less than 1, place a 0 in the 1 position</b>]]></text>
</component>

<component id="ID_clip1_txt14">
   <text><![CDATA[<b>ALL DONE</b>]]></text>
</component>

<component id="ID_clip1_txt15">
   <text><![CDATA[Answer: <b>172 = 10101100</b>]]></text>
</component>

